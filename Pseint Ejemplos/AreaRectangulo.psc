Algoritmo AreaRectangulo
	
	//Definir variables
	Definir base como Real;
	Definir altura como Real;
	Definir area como Real;
	//Entrada
	Escribir "Inserte la base del rectangulo";
	Leer base;
	Escribir "Inserte la altura del rectangulo";
	Leer altura;
	//Proceso 
	area = base*altura;
	//Salida
	Imprimir "El area del rectangulo es: ", area;
FinAlgoritmo
