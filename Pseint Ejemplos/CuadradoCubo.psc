Algoritmo CuadradoCubo
	
	
	Definir num Como Real;
	Definir cuadrado Como Real;
	Definir cubo como Real;
	Imprimir "Inserte un número";
	Leer num;
	
	cuadrado = num*num;
	cubo = num*num*num;
	
	
	Imprimir "El cuadrado de " num " es: " cuadrado;
	Imprimir "--------------------------------------";
	Imprimir "El cubo de " num " es: " cubo;
	
	
FinAlgoritmo
