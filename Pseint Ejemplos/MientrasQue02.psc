Algoritmo MientrasQuePrueba02
	//Definir variables
	Definir x como entero;
	
	Escribir "Proporcione un numero entero";
	leer x;
	
	//Proceso y salida
	Mientras x > 0 Hacer
		Escribir "El numero es positivo";
		Escribir "Proporcione un numero entero";
		leer x;
	Fin Mientras
	Escribir "El numero es negativo";
FinAlgoritmo
